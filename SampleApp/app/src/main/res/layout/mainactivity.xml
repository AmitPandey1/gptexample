                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/textView2"
                android:layout_alignParentBottom="true"
                android:layout_toLeftOf="@+id/editText5"
                android:ems="10"
                android:hint="name"
                android:inputType="text" />

            <EditText
